{
    "name": "lstrojny/fxmlrpc",
    "description": "Fast and tiny XML/RPC client with bridges for various HTTP clients",
    "keywords": ["xmlrpc", "xml", "api", "rpc", "performance"],
    "license": "MIT",
    "authors": [
        {
            "name": "Lars Strojny",
            "email": "lstrojny@php.net",
            "homepage": "http://usrportage.de"
        }
    ],
    "require": {
        "php": ">=5.4",
        "php-http/httplug": "~1",
        "php-http/discovery": "^0.8.0",
        "php-http/message-factory": "^1.0"
    },
    "require-dev": {
        "zendframework/zend-log": "~2",
        "zendframework/zendframework1": "~1.12.4",
        "monolog/monolog": ">=1.1",
        "psr/log": "*",
        "phpunit/phpunit": "~4",
        "symfony/process": "~2.1",
        "lstrojny/hmmmath": "dev-master",
        "internations/kodierungsregelwerksammlung": "dev-master",
        "guzzlehttp/guzzle": "^6.2",
        "php-http/message": "^1.2",
        "php-http/guzzle6-adapter": "^1.1"
    },
    "suggest": {
        "zendframework/zend-log": "To use ZF2 Zend\\Log\\Logger as a logger",
        "zendframework/zendframework1": "To use ZF1 Log",
        "monolog/monolog": "To integrate monolog as logger",
        "php-http/message": "For PSR-7 compatible message factories",
        "php-http/curl-client": "A simple PSR-7 compatible HTTP client"
    },
    "autoload": {
        "psr-4": {"fXmlRpc\\": "src/fXmlRpc"}
    },
    "autoload-dev": {
        "psr-4": {"fXmlRpc\\": "tests/fXmlRpc"}
    },
    "extra": {
        "branch-alias": {"dev-master": "1.0-dev"}
    }
}
